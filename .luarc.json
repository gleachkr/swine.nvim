{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "diagnostics": {
    "globals": [
      "vim"
    ]
  },
  "workspace": {
    "checkThirdParty": false,
    "ignoreDir": [
      ".direnv",
      ".git"
    ],
    "library": [
      "${3rd}/luv/library",
      "${3rd}/busted/library"
    ]
  },
  "telemetry": {
    "enable": false
  }
}
